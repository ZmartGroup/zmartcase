<div class="col-sm-5">
  <h1>Priorities</h1>
  <table class="table table-striped">
    <tr>
      <th>#</td>
      <th>Name</td>
      <th>Created at</td>
    </tr>
    <% @prios.each do |prio| %>
      <tr>
        <td><%= prio.id %></td>
        <td><%= prio.name %></td>
        <td><%= prio.created_at %></td>
      </tr>
    <% end %>
  </table>

  <%= link_to "New priority", new_priority_path, class: "btn btn-primary" %>
</div>